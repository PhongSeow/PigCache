<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>PigKeyCacheCoreLib.SQLServerDemo</RootNamespace>
    <TargetFrameworks>net5.0;netcoreapp3.1;</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Seow Phong</Authors>
    <Company>Seow Phong Studio</Company>
    <Description>Demo console program for PigKeyCacheCoreLib.SQLServer</Description>
    <Copyright>Copyright (c) 2021-2022 Seow Phong</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://en.seowphong.com/oss/PigCache/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/PhongSeow/PigCache/tree/master/Src/PigKeyCacheCoreLib.SQLServerDemo</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageTags>Key Value Windows cache Single process Multithreading SQLServer Linux Core</PackageTags>
    <Version>3.1.12</Version>
    <PackageReleaseNotes>Refer to PigConsole.Getpwdstr of PigCmdLib  is used to hide the entered password.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="PigCmdLib" Version="1.1.20" />
    <PackageReference Include="PigKeyCacheCoreLib.SQLServer" Version="3.1.16" />
  </ItemGroup>

</Project>
